@{
    ViewBag.Title = "Home";
}

<h1>@ViewBag.Title</h1>

<p>Welcome to VisualBitcoin website.</p>

<script>
    $(function () {
        $("#accordion").accordion({ collapsible: true });
    });
</script>

@model List<Storage.Models.Transactions>

<div id="accordion">
@{foreach (Storage.Models.Transactions modelItem in Model)
    {
        <h3> <a href="#">@Html.DisplayFor(modelItemProperty => modelItem.Locktime)</a></h3>
        <div>
            <table>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => modelItem.Txid)
                    </th>
                </tr>
            </table>
    </div>
    }
}
</div>


